@model Domain.Entities.Property

<div class="card">
    <div class="card-body">
        <h5 class="card-title">@Model.Name</h5>
        <p class="card-text">@Model.Blurb</p>
        <a class="btn btn-primary"
           asp-controller="PropertyListing"
           asp-action="ViewPropertyDetails"
           asp-route-id="@Model.Id">
            Details
        </a>
    </div>
</div>
<br />
